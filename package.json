  "repository": {
    "type": "git",
    "url": "https://github.com/kieronwiltshire/node-respondent.git"
  },
  "bugs": {
    "url": "https://github.com/kieronwiltshire/node-respondent/issues"
  },
  "homepage": "https://github.com/kieronwiltshire/node-respondent#readme",
